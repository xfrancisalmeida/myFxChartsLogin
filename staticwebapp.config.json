{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "openIdIssuer": "https://login.microsoftonline.com/37e96f4f-82ed-4688-a652-2b096de4e630"
        }
      }
    },
    "routes": [
      {
        "route": "/protected",
        "allowedRoles": ["authenticated"],
        "unauthenticatedAction": "RedirectToLoginPage"
      },
      {
        "route": "/*",
        "allowedRoles": ["authenticated"],
        "unauthenticatedAction": "RedirectToLoginPage"
      }
    ]
  },
  "routes": [
    {
      "route": "/protected",
      "rewrite": "/index.html"
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
